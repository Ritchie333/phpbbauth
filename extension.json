{
    "name": "phpBB Auth",
    "version": "0.0.1",
    "author": [
        "[https://multidimension.al/ multidimension.al]"
    ],
    "url": "https://github.com/multidimension-al/phpbbauth",
    "descriptionmsg": "Automatically logs users in/out using their phpBB session. Requires the Auth_remoteuser extension version 2.0+.",
    "license-name": "MIT",
    "type": "mediawiki-extension",
    "config": {
        "_prefix": "wgPhpBBAuth",
        "ForumDirectory": "./../phpBB3/"
    },
    "requires": {
        "MediaWiki": ">= 1.27.0",
        "extensions": {
            "Auth_remoteuser": ">= 2.0.0"
        }
    },
    "AutoloadClasses": {
        "PhpbbAuthHooks": "includes/PhpbbAuthHooks.php"
    },
    "Hooks": {
        "PersonalUrls": "PhpBBAuthHooks::onPersonalUrls"
    },
    "manifest_version": 1
}