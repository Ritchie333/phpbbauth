{
    "name": "phpBB Auth",
    "version": "0.0.1",
    "author": [
        "multidimension.al"
    ],
    "url": "",
    "descriptionmsg": "Automatically logs users in/out using their phpBB session. Requires the Auth_remoteuser extension version 2.0+.",
    "license-name": "MIT",
    "type": "mediawiki-extension",
    "config": {
        "_prefix": "wgPhpBBAuth",
        "ForumDirectory": "./../phpBB3/"
    },
    "requires": {
        "MediaWiki": ">= 1.27.0",
        "extensions": {
            "Auth_remoteuser": ">= 2.0.0"
        }
    },
    "Hooks": {
        "PersonalUrls": "PhpBBAuth:onPersonalUrls"
    },
    "AutoloadClasses": {
        "PhpBBAuth": "PhpBBAuth.php"
    }
}